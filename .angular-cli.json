{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "glaucoma-risk-calculator-web-frontend"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap-reboot.min.css",
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/quill/assets/bubble.styl",
        "../node_modules/quill/assets/snow.styl",
        "../src/assets/fonts/css/animation.css",
        "../src/assets/fonts/css/fontello-codes.css",
        "../src/assets/fonts/css/fontello.css",
        "../src/assets/fonts/css/fontello-embedded.css",
        "../src/assets/fonts/css/fontello-ie7-codes.css",
        "../src/assets/fonts/css/fontello-ie7.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/mathjs/dist/math.min.js"
      ],
      "addons": [
        "../src/assets/fonts/font/*.+(otf|eot|svg|ttf|woff|woff2)"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
